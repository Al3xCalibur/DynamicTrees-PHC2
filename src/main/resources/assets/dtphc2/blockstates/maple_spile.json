{
  "multipart": [
    {   "when": { "facing": "south" },
      "apply": { "model": "dtphc2:block/maple_spile_empty" }
    },
    {   "when": { "facing": "west" },
      "apply": { "model": "dtphc2:block/maple_spile_empty", "y": 90 }
    },
    {   "when": { "facing": "north" },
      "apply": { "model": "dtphc2:block/maple_spile_empty", "y": 180 }
    },
    {   "when": { "facing": "east" },
      "apply": { "model": "dtphc2:block/maple_spile_empty", "y": 270 }
    },
    {   "when": { "facing": "south", "filled": "true" },
      "apply": { "model": "dtphc2:block/maple_spile_syrup_stream" }
    },
    {   "when": { "facing": "west", "filled": "true" },
      "apply": { "model": "dtphc2:block/maple_spile_syrup_stream", "y": 90 }
    },
    {   "when": { "facing": "north", "filled": "true" },
      "apply": { "model": "dtphc2:block/maple_spile_syrup_stream", "y": 180 }
    },
    {   "when": { "facing": "east", "filled": "true" },
      "apply": { "model": "dtphc2:block/maple_spile_syrup_stream", "y": 270 }
    }
  ]
}