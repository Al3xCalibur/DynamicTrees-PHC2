{
  "multipart": [
    {   "when": { "distance": "1|2" },
      "apply": { "model": "dtphc2:block/palm_leaves/banana_frond" }
    },
    {   "when": { "distance": "3" },
      "apply": { "model": "dtphc2:block/palm_leaves/banana_core_top" }
    },
    {   "when": { "distance": "4" },
      "apply": { "model": "dtphc2:block/palm_leaves/banana_core_bottom" }
    },
    {   "when": {"OR":[ { "direction": "0", "distance": "1|2" }, { "distance": "5|6|7" } ]},
      "apply": { "model": "block/jungle_leaves" }
    }
  ]
}